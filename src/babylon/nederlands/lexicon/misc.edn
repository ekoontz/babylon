;; 'F.' means Carol Fehringer, "A Reference Grammar of Dutch", 1999 ,Cambridge University Press.
{"behoorlijk"
 [{:cat :intensifier
   :sem {:pred :quite}
   :subcat {:1 {:cat :adjective
                :phrasal false}
            :2 []}}]

 "dat" [{:cat :det
         :definite? true
         :sem {:pred :that}
         :agr {:number :sing
               :gender :neuter}}]
        
 ;; <F. p30-31, 54-55, 70>
 "de" [{:cat :det
        :definite? true
        :sem {:pred :the}
        :agr {:gender :common}}
       {:cat :det
        :definite? true
        :sem {:pred :the}
        :agr {:number :plur}}]

 "deze" [{:cat :det
          :definite? true
          :sem {:pred :this}
          :agr {:number :sing
                :gender :common}}
         {:cat :det
          :definite? true          
          :sem {:pred :this}
          :agr {:number :plur}}]

 "die" [{:agr {:number :plur
               :person :3rd}
         :case :acc
         :cat :noun
         :pronoun true
         :sem {:pred :they
               :ref {:human false}}}
        {:cat :det
         :definite? true
         :sem {:pred :that}
         :agr {:number :sing
               :gender :common}}
        {:cat :det
         :definite? true
         :sem {:pred :that}
         :agr {:number :plur}}]

 "dit" [{:cat :det
         :definite? true
         :sem {:pred :this}
         :agr {:number :sing
               :gender :neuter}}]

 "echt" [{:cat :intensifier
          :sem {:pred :really}
          :subcat {:1 {:cat :adjective
                        :phrasal false}
                    :2 []}}]
 "een" [{:cat :det
         :definite? false
         :sem {:pred :some}
         :agr {:number :sing}}]

 "en" [{:cat :conjunction
        :sem {:pred :and}}]

 "erg" [{:cat :intensifier
         :sem {:pred :very}
         :subcat {:1 {:cat :adjective
                      :phrasal false}
                  :2 []}}]
 "eigenlijk" [{:cat :intensifier
               :sem {:pred :actually}
               :subcat {:1 {:cat :adjective
                            :phrasal false}
                        :2 []}}]
 "geen" [{:cat :det
          :definite? false
          :sem {:pred :none}
          :agr {:number :sing}}]

 "haar" [{:agr {:gender :fem
                :number :sing
                :person :3rd}
          :case :acc
          :cat :noun
          :pronoun true
          :sem {:pred :she
                :ref {:human true}}}
         {:cat :det
          :definite? true
          :sem {:pred :she}}]
 
 "heel" [{:cat :intensifier
          :sem {:pred :very}
          :subcat {:1 {:cat :adjective
                        :phrasal false}
                    :2 []}}]

 "hem" [{:agr {:gender :masc
               :number :sing
               :person :3rd}
         :case :acc
         :cat :noun
         :pronoun true
         :sem {:pred :he
               :ref {:human true}}}]

 "hen" [{:agr {:number :plur
               :person :3rd}
         :case :acc
         :cat :noun
         :pronoun true
         :sem {:pred :they
               :ref {:human true}}}]

 "het" [{:cat :det
         :definite? true
         :sem {:pred :the}
         :agr {:gender :neuter
               :number :sing}}
        {:cat :noun
         :pronoun true
         :sem {:pred :it
               :ref {:human false}}
         :agr {:person :3rd
               :number :sing
               :gender :masc}
         :case :nom}]
 
 ;; <F. p69-73>
 "hij" [{:agr {:person :3rd
               :number :sing
               :gender :masc}
         :case :nom
         :cat :noun
         :pronoun true
         :sem {:pred :he
               :ref {:human true}}}]

 "hun" [{:agr {:number :plur
               :person :3rd}
         :case :acc
         :cat :noun
         :pronoun true
         :sem {:pred :they
               :ref {:human true}}}
        {:cat :det
         :definite? true
         :sem {:pred :they}}]

 "ik" [{:agr {:person :1st
              :number :sing}
        :case :nom
        :cat :noun
        :pronoun true
        :sem {:pred :i
              :ref {:human true}}}]

 "je" [{:cat :noun
        :case :acc
        :agr {:person :2nd}
        :pronoun true
        :reflexive true
        :sem {:ref {:human true
                    :context :informal}}}]

 "jij" [{:agr {:person :2nd
               :number :sing}
         :case :nom
         :cat :noun
         :pronoun true
         :sem {:pred :you
               :ref {:human true
                     :context :informal}}}]

 "jou" [{:agr {:person :2nd
               :number :sing}
         :case :acc
         :cat :noun
         :sem {:pred :you
               :ref {:human true}}
         :pronoun true}]

 "jouw" [{:cat :det
          :definite? true                 
          :sem {:pred :you
                :context :informal}}]

 "jullie" [{:case :nom
            :cat :noun
            :pronoun true
            :sem {:pred :you
                  :ref {:human true
                        :context :informal}}
            :agr {:person :2nd
                  :number :plur}}]

 "me" [{:cat :noun
        :case :acc
        :agr {:number :sing
              :person :1st}
        :sem {:ref {:human true}}
        :pronoun true
        :reflexive true}]

 "mij" [{:case :acc
         :cat :noun
         :pronoun true
         :sem {:pred :i
               :ref {:human true}}
         :agr {:person :1st
               :number :sing}}]

 "mijn" [{:cat :det
          :definite? true
          :sem {:pred :i}}]

 "ongewoon" [{:cat :intensifier
              :sem {:pred :unusually}
              :subcat {:1 {:cat :adjective
                           :phrasal false}
                       :2 []}}]
 
 "ons" [{:cat :noun
         :case :acc
         :agr {:number :plur
               :person :1st}
         :pronoun true
         :reflexive true}
        {:cat :det
         :definite? true
         :agr {:number :sing
               :gender :neuter}
         :sem {:pred :we}}]

 "onze" [{:cat :det
          :agr {:number :sing
                :gender :common}
          :definite? true
          :sem {:pred :we}}
         {:cat :det
          :definite? true
          :agr {:number :plur}
          :sem {:pred :we}}]
 
 ;; "u" is very generic: can be
 ;; sing/plur and nom/acc.
 "u" [{:agr {:person :2nd}
       :cat :noun
       :pronoun true
       :sem {:pred :you
             :ref {:context :polite}}}
      {:cat :noun
       :case :acc
       :agr {:person :2nd}
       :pronoun true
       :reflexive true
       :sem {:ref {:context :polite}}}]

 "uw" [{:cat :det
        :definite? true
        :sem {:pred :you
              :context :polite}}]


 ;; TODO: any difference in usage between 'we' and 'wij'?
 "we" [{:agr {:person :1st
              :number :plur}
        :case :nom
        :cat :noun
        :pronoun true
        :sem {:pred :we}}]

 "wij" [{:agr {:person :1st
               :number :plur}
         :case :nom
         :cat :noun
         :pronoun true
         :sem {:pred :we}}]

 ;; TODO: F.95: 'zich' can also be used for {:person :2nd}
 ;; to avoid repetition of 'u' (e.g. 'wilt u zich aankleden?')
 "zich" [{:cat :noun
          :case :acc
          :agr {:person :3rd}
          :pronoun true
          :reflexive true}]

 "zij" [{:agr {:person :3rd
               :number :sing
               :gender :fem}
         :case :nom
         :cat :noun
         :pronoun true
         :sem {:pred :she
               :ref {:human true}}}
        {:agr {:person :3rd
               :number :plur}
         :case :nom
         :cat :noun
         :pronoun true
         :sem {:pred :they
               :ref {:human true}}}]

 "zijn" [{:cat :det
          :definite? true
          :sem {:pred :he}}

         {:cat :det
          :definite? true
          :sem {:pred :it}}]}


