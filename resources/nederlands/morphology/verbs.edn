 {:g [#"^(.*s)sen$" "$1ten"]
  :p [#"^(.*s)ten$"  "$1sen"]
  :u {:cat :verb
      :agr {:number :plur}
      :infl :past-simple}}

 ;; single consonants
 {:g [#"^(.*[tksp])en$" "$1te"]
  :p [#"^(.*[tksp])te$" "$1en"]
  :u {:cat :verb
      :agr {:number :sing}
      :infl :past-simple}}

 {:g [#"^(.*[tksp])en$"  "$1ten"]
  :p [#"^(.*[tksp])ten$" "$1en"]
